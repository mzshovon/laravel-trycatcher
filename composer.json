{
    "name": "mzshovon/laravel-trycatcher",
    "description": "A lightweight exception guard for Laravel to simplify try-catch handling with logging and policies.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mzshovon\\LaravelTryCatcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mzshovon\\LaravelTryCatcher\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Zaman Shovon",
            "email": "zaman.monir20188@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "8.x-dev"
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Mzshovon\\LaravelTryCatcher\\ExceptionGuardServiceProvider"
            ],
            "aliases": {
                "Guard": "Mzshovon\\LaravelTryCatcher\\Facades\\Guard"
            }
        }
    }
}
